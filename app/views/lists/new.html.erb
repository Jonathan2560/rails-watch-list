<div class="container">
  <div class="row justify-content-center mt-4">
    <div class="col-4">
      <h1>Add a new list</h1>
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
